      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "memo",
      "styles": [
        "styles.scss",
        "theme.scss",
        "spinner.scss",
        "mixins.scss"
